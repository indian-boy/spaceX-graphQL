    "@emotion/styled": "11.10.5",
    "framer-motion": "8.2.0",
    "graphql": "16.6.0",
    "next": "13.1.1",
    "next-pwa": "5.6.0",
    "next-seo": "5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.42.0",
    "react-icons": "4.7.1",
    "react-intersection-observer": "9.4.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.4.0",
